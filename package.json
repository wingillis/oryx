{
  "name": "oryx",
  "version": "0.0.0",
  "description": "Markdown note taking app inspired by bear",
  "main": "app.js",
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e main.js -o dist/build.js",
    "serve": "http-server -o -c 1 -a localhost",
    "test": "mocha",
    "dev": "npm-run-all --parallel watchify serve"
  },
  "keywords": [
    "markdown",
    "notes",
    "editor"
  ],
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "author": "wingillis",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-hmr": "^0.3.5",
    "codemirror": "^5.29.0",
    "mocha": "^3.5.0",
    "npm-run-all": "^4.1.1",
    "uuid": "^3.1.0",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "markdown-it": "^8.4.0",
    "pouchdb": "^6.3.4",
    "vue": "^2.4.2"
  }
}
